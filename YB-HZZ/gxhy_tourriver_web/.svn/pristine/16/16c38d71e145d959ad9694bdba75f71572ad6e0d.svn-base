.yuan{ width:2rem; height:2rem; margin:0 auto; box-sizing:border-box; padding-top:0.2rem; text-align:center; background-color:#f1f1f1; border-radius:50%; position:relative;}
.yuan_bl1 , .yuan_bl2 , .yuan_bl3 , .yuan_bl4{ width:100%; height:100%; border-radius:50%; position:absolute; left:0; top:0;}
.yuan_bl1 , .yuan_bl2{ background-color:#FF7F4C; -webkit-box-shadow:0 0 0.1rem 0.1rem #fff inset; box-shadow:0 0 0.1rem 0.1rem #fff inset;}
.yuan_bl3 , .yuan_bl4{ background-color:#f1f1f1;}
.yuan_bl1 , .yuan_bl3{ clip:rect(0 2rem 1rem 0);}
.yuan_bl2 , .yuan_bl4{ clip:rect(1rem 2rem 2rem 0);}
.yuan_text{ width:1.6rem; height:1.6rem; line-height:1.6rem; box-sizing:border-box; padding-left:0.1rem; margin:0 auto; color:#FF7F4C; font-size:0.36rem; font-family:"PingFangSC-Thin","sans-serif","STHeitiSC-Light","Î¢ÈíÑÅºÚ","Microsoft YaHei"; background-color:#fff; border-radius:50%; position:relative;}
