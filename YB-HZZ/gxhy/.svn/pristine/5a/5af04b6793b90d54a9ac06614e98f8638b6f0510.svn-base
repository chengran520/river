.title-div{
  border-bottom: 1px solid #45515e;
}
.layui-tab-title {
	height: 0.52rem !important;
    line-height: 0.52rem !important;
	opacity: 1 !important;
}
.layui-tab-title li {
	width: 1.3rem !important;
	background-color: #ecf1f0 !important;
	height: 0.52rem !important;
	line-height: 0.52rem !important;
	font-size: 0.18rem;
	font-family: MicrosoftYaHei;
	color: #7F7F7F;
	text-align: center;
}
.layui-tab-title .layui-this:after {
	height:0.52rem !important;
}
.layui-tab-title .layui-this{
	color:#0086CF;
	background:rgba(173,228,255,1) !important;	
}
.layui-tab-title {
	border-bottom: 2px solid #e0e1e1 !important;
}

.layui-tab-card>.layui-tab-title .layui-this:after {
	border-width: 0px !important;
}
.layui-tab-card>.layui-tab-title li {
	margin-right: 0.04rem !important;
}
.layui-table-box{
	padding-left:0;
	padding-right:0;
	width:auto;
}
.layui-tab-content {
	padding: 0px;
}
.tab-search {
	font-size: 0.22rem;
	margin-left: 0.25rem;	
    height: 0.6rem;
    line-height: 0.64rem;
    padding-right:0.25rem;
}
.tab-search span {
	margin-left: 0.04rem;
	color: #007BBE;
}
.layui-btn{
    display: inline-block;
    box-sizing: border-box;
    text-align: center;
    font-weight: 400;
    vertical-align: middle;
    border: solid 1px #ddd;
    width: auto;
    font-size: 0.16rem;
    padding: 0px 0.09rem !important; 
    border-radius: 4px;
    font-family:MicrosoftYaHei;
	letter-spacing: 1px;
	height: 0.36rem;
   line-height: 0px;
}
.btn-sel{
    color: #00DDE4;
    border:1px solid #00DDE4;
    background:#FCFCFC;

}
.btn-sel:hover{
	background-color:#00DDE4;
	color:#fff;
}
.btn-imp{
	color:  #d400e7 ;
    border:1px solid  #d400e7;
    background:#fef8ff;
}
.btn-imp:hover{
	background-color:#d400e7;
	color:#fff;
}
.btn-exp{
	color:#0095E7;
	border:1px solid #0095E7;
	background:#F4FBFF; 
}
.btn-exp:hover{
	background-color:#0095E7;
	color:#fff;
}
.btn-del{
	color:#FF6366;
	border:1px solid #FF6366;
	background:#FFF3F3; 
}
.btn-del:hover{
	background-color:#FF6366;
	color:#fff;
}
.btn-add{
	background-color:#FFF3EF;
	border:1px solid #F17F1A;
	color:#F17F1A;
}
.btn-add:hover{
	background-color:#F17F1A;
	color:#fff;
}
.btn-updata{
	background-color:#FFF3EF;
	border:1px solid #7CCD7C;
	color:#7CCD7C;
}
.btn-updata:hover{
	background-color:#7CCD7C;
	color:#fff;
}
.btn-pwd{
	color:#FF6366;
}
.btn-setRole,.btn-authorize{
	color:#0095E7;
}
.btn-edit{
	color:#F17F1A;
}
.btn-pwd,.btn-setRole,.btn-authorize,.btn-edit{
	font-weight: 700;
	font-family:MicrosoftYaHei;
	letter-spacing: 1px;
}
.btn-setRole:hover,.btn-authorize:hover,.btn-edit:hover{
	opacity:0.8;
	cursor: pointer;
}
.seach-input {
	width: 2.76rem;
	border-radius: 4px;
	border: 1px solid #CCCCCC;
	box-shadow:0px 3px 0.09rem 0px #DBDBDB;
    font-size: 0.16rem;
	font-family:MicrosoftYaHei;
	background: #FCFCFC;
	box-shadow:3px 0.53rem 3px #DBDBDB inset; 
}
 .seach-input::-webkit-input-placeholder {
	color: #949494;
}
.seach-input:-moz-placeholder { 
	color: #949494;
}

.seach-input::-moz-placeholder {
	color: #949494;
}

.seach-input:-ms-input-placeholder {
	color: #949494;
}
