.layui-body{	
	font-size:0.14rem;
	background-color: #e6e8ef !important;
}
.layui-side {
    top: 1.1rem;
    width: 2.32rem;
    display:none;
    overflow-x: hidden;
}
.layui-side-scroll{
	 width: 2.22rem;
	 background-color: #010a1c !important;
}
.layui-nav-tree {
	width: 2.22rem; 
}
#contract{
	position: fixed;
	left:2.22rem;
	width:0.1rem;
	height:100%;	
}
#contract img{
	position: fixed;
    top: 2.46rem;
    left: 2.22rem;
}
#contract img:hover{
	cursor: pointer;
}
.layui-bg-black {
	background-color: #d7d9e0 !important;
}
.layui-nav-tree .layui-nav-bar {
    width: 0px !important;
    height: 0;
}
 .layui-nav-tree .layui-nav-child dd.layui-this, .layui-nav-tree .layui-nav-child dd.layui-this a,
	.layui-nav-tree .layui-this, .layui-nav-tree .layui-this>a,
	.layui-nav-tree .layui-this,.layui-nav-tree .layui-nav-item>a:hover{
	background-color: #00dee4 !important;
	cursor: pointer;
}
.layui-nav-tree .layui-nav-item a:hover .layui-icon.layui-icon-right{
	 color: #fff;
}
.layui-nav-item a {
	font-size: 0.16rem !important;
}
.layui-icon-right {
    float: right;
    margin-right: 0.2rem;
    color: #575e6b;
    font-size: 0.18rem;
}
#currentNm{
	color:#F9D90B;
	font-size: 0.16rem;
}
#currentNm:hover{
	color:#F9D90B;
}
.layui-body{
	background-color:#fff!important;
	position: fixed;
	left: 2.32rem;
	right: 0;
	bottom:0px !important;
	height: auto;
    overflow: auto;
}
