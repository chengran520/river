	font-size:0.15rem;
	text-align: center;
	height: 0.25rem;
	line-height: 0.25rem;
}
.p1{
	cursor: pointer;
}
.p2{
	color:red;
	font-size:0.13rem;
}
.layui-layer-lan .layui-layer-title {
	background: #00dde4 !important;
	text-shadow:#010a1c 1px 1px 3px;
	font-size:0.16rem;
}
.layui-layer-lan .layui-layer-btn .layui-layer-btn0 {
    background: #00dde4 !important;
    border-color: #00dde4 !important;
    color: #fff !important;
}
.layui-layer-lan .layui-layer-btn .layui-layer-btn1 {
    background: #fff !important;
    border-color: #00dde4 !important;
}
.layui-layer-lan .layui-layer-btn:hover{
	opacity:0.8;
}
.layui-tab-card{
  box-shadow: 0 0px 0px 0 rgba(0, 0, 0, 0);
}
 .layui-table thead tr {
    background-color: #FFF!important;
} 
.layui-table tr:nth-child(odd) {
    background-color: #E2F1FD;
}
.layui-table tbody tr:hover{
	background-color: #c2e1fb!important;
}

.layui-table-page {
     background: #E2F1FD;
    left: 0;
    right: 0;
    width: 100%;
}
.layui-laypage .layui-laypage-curr .layui-laypage-em {
    background-color: #00a0e9;
}
