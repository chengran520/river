	//获取河道信息
	initData:function(key){
		this.data = [];
		$("#inpt_query").show();
		$("#all_div").css("top","40px");
		//isTp:0 查询河流，1查询测站，3查询行政区
		//获取河流
		var data = {'rvNm':key};
		var reulst = commonUtils.get(WEB_ROOT+"/data/monitor/riverDataInfo",data,false);
		if(reulst.flag){
			 for(var i = 0 ;i < reulst.data.length;i++){
				 this.data.push(reulst.data[i]);
			 }
			 this.rvData= reulst.data;
			 this.showDataDiv();
		}
	},
	showDataDiv:function(){
		var data = this.rvData;
		var html = "<ul class='well' id='well_div' style='overflow: auto;height: 6.8rem;margin: 5px auto 0;'>";
		if(data.length > 0){
			for(var i = 0 ;i < data.length;i++){
				html += "<li><div><span style='display:none' id='rv_stcd'>"+data[i].rvCd+"</span><span class='colorGreen'>"+data[i].rvNm+"</span><div style='clear: both;'></div>";
				html += "<p class='view_span_defalut'>上级河流："+(data[i].prvNm == null ? "暂无":data[i].prvNm)+"</p><p class='view_span_defalut'>河流长度："+(data[i].rvlt == null ? "暂无":(data[i].rvlt)+"km")+"</p></div>";
				html += "</li>";
			}
		}
		html += "</ul>";
		$("#stationList_defaultView").html(html);
	},
	 drawArea:function(json){
		var area = json.area; 
		var points = area.points; 
		//如果有区域坐标信息，画出此行政区的多边形。 
		if(points && points.length != 0){ 
	        for(var i = 0;i < points.length;i++){ 
				var region = points[i].region;//单个面 
				var pointArr = region.split(","); 
				var pointsz = []; 
				for(var j = 0;j < pointArr.length - 1;j++){ 
					var p = pointArr[j]; 
					var pArr = p.split(" "); 
					var point = new T.LngLat(pArr[0],pArr[1]); 
					pointsz.push(point);				 
				} 
				//面的线的样式 
				 var poly = new T.Polygon(pointsz, {
                     color: "rgb(255, 255, 0)",
                     fillColor:"#AFEEEE",
                     weight: 5,
                     opacity: 1,
                     fillOpacity:0,
                     lineStyle: "solid"
                 });
				this.map.addOverLay(poly); 
			} 
		} 
	},
	riverData:function(type,vcd){
		var lineArr = [this.qingjiangLine,this.qtsLine,this.hjhLine,this.dalongxiLine,this.xlongxiLine,this.huangshaLine,this.xiningLine,this.yuexiLine,this.changningLine,this.nanguangheLine,this.songjiangheLine,this.lishuiheLine];
		var Linew = 5;
		var linColor = "#0080FF";
		for (var i = 0,aLen = lineArr.length; i < aLen; i++) {
			var tp = lineArr[i].linetp;
			var rvCd = lineArr[i].riverCd;
			if(tp == 11){
				Linew = 4;
			}
			if(tp == 22){
				Linew = 3;
			}
			if(type == 1){
				if(rvCd === vcd){
					linColor = "red";
					Linew = 6;
					this.checkOverlayTp();
					this.drawLine(lineArr[i],Linew,linColor);
					return;
				}
			}else{
				this.drawLine(lineArr[i],Linew,linColor);
			}
		};
	},
	checkOverlayTp:function(){
		var overlay = TiandtMap.map.getOverlays();
		if(this.overlays == null){			
			this.overlays = overlay;
		}
		if(this.overlays.length > 0){
			for(var i =0 ;i < this.overlays.length;i++){
				var tp = this.overlays[i].options.tp;
				if(tp == "line"){
					TiandtMap.map.removeOverLay(this.overlays[i]);
				}
			}
		}
	},
	drawLine:function(json,Linew,linColor){
		var line = json.line; 
		var points = line.points; 
		var rvNm = json.riverNm;
		var rvCd = json.riverCd;
		var flag = this.flagRiverInfo(this.rvData,rvCd);
		//如果有区域坐标信息，画出此行政区的多边形。 
		if(flag && points && points.length != 0){ 
	        for(var i = 0;i < points.length;i++){ 
				var region = points[i].region;//单个面 
				var pointArr = region.split(","); 
				var pointsz = []; 
				var pointLine = [];
				for(var j = 0;j < pointArr.length - 1;j++){ 
					var p = pointArr[j]; 
					var pArr = p.split(" "); 
					var point = new T.LngLat(pArr[0],pArr[1]); 
					pointsz.push(point);
					if(j == parseInt(pointArr.length/2)){
						 pointLine = point;
						 var lactionR = {};
						 lactionR.rvcd=rvCd;
						 lactionR.point=point;
						 //this.map.addOverLay(label);
						 this.LocationRiver.push(lactionR);
					}
				} 
				//线的样式 
				this.polyLine = new T.Polyline(pointsz, {
                     color: linColor,
                     weight: Linew,
                     opacity: 1,
                     lineStyle: "solid",
                     tp:'line',
                     rvCd:rvCd
                 });
				 this.map.addOverLay(this.polyLine); 
			} 
		} 
	},
	flagRiverInfo:function(data,rvcd){
		if(data != null){
			for(var i = 0 ;i < data.length ;i++){
				if(data[i].rvCd == rvcd){
					return true;
				}
			}
		}
		return false;
	},
