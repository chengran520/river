.layui-fluid {
    padding: 0.3rem 0.4rem 0px 0.3rem;
}
.layui-card{
	border-radius:5px;
	height:3.4rem !important;
    background:#fff;
}
.layui-card-header{
	height: 0.52rem;
}
.layui-card-header{
	font-size: 0.18rem;
    font-family: MicrosoftYaHei;
    font-weight: bold;
    color: rgba(127, 125, 125, 1);
    line-height:0.52rem;
    border-bottom:1px solid rgba(180,180,180,0.3);
    box-shadow: 0px 1px 0px rgba(180,180,180,0.1);
}
.layui-card .layui-card-header span{
	font-size: 0.14rem;
    font-family: MicrosoftYaHei;
    color: #fff;
    padding: 0px 0.1rem;
    background:rgba(136,149,169,0.9);
    border-radius: 7px;
    font-weight: 400;
    cursor: pointer;
}
.layui-card .layui-card-header span:hover,.layui-card .layui-card-header span:first-child,.spanActive{
	  background:#666666bf;
}
.rate{
	padding: 0.3rem 0.2rem 1rem 0.2rem;
	font-size:0.2rem;
	font-family:MicrosoftYaHei;
	font-weight:400;
	color:#8895A9;
}
.rate p{
	height: 0.3rem;
	padding-top: 0.06rem;
}
#qualiteRate,#eventRate{
	font-size: 0.33rem;
}
#qualiteRate{
	color: #F36A5B;
}
#eventRate{
	color: #5C9BD1;
}
.layui-col-md12{
	height: 3.6rem;
}
.layui-col-md12 .count-title{
	height:0.5rem;
	font-size:0.25rem;
	font-family:MicrosoftYaHei;
	font-weight:400;
	color:rgba(0,123,190,1);
	line-height:0.5rem;
}
.layui-table-box{
	padding:0px;
	overflow-x:hidden;
}
.countDetail{
	color:#138CCF;
}
.layui-form-item .layui-input-inline {
	padding:0px 0.09rem;
    width: 2.4rem;
    line-height: 0.36rem;
}
.layui-form-label{
	padding:0.09rem 0.05rem;
}
.lable_div_before {
    width: 0.87rem;
    height: 0.35rem;
    background: rgba(243,251,255,1);
    border: 1px solid rgba(190, 226, 244, 1);
    font-size: 0.14rem;
    float: left;
    line-height: 0.35rem;
    text-align: center;
    font-family: MicrosoftYaHei;
    border-radius: 3px;
}
.menu_select {
    width: 1.74rem;
    height: 0.37rem;
    background: rgba(255,255,255,1);
    border: 1px solid rgba(190, 226, 244, 1);
    font-size: 0.14rem;
    float: left;
    border-radius: 3px;
}
.downrightheader_div{
	width: 100%;
	height: 10%;
	border-bottom: solid 2px #ccc;
}
.downrightheader_div span{
	margin-left: 0.04rem;
    color: #4db7f1;
    line-height: 0.5rem;
    font-size: 0.18rem;
    margin-left: 0.1rem;
}
.rightBottom_div{
	background-color: #fff;
	width: 30%;
    float: right;
}
.layui-table-page{
	width: 100%!important;
	left:0!important;
}
.inside_div{
	width: 49%;
	height: 49%;
	float: left;
	padding: .5%;
	text-align: center;
}
.inside_yellow_span{
	font-size: 0.25rem;
	color: #d8d807f7;
}
.inside_blue_span{
	font-size: 0.25rem;
	color: blue;
}
.inside_green_span{
	font-size: 0.25rem;
	color: green;
}
.inside_orange_span{
	font-size: 0.25rem;
	color: orange;
}